{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "my-portfolio": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/my-portfolio",
      "sourceRoot": "projects/my-portfolio/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/my-portfolio",
            "index": "projects/my-portfolio/src/index.html",
            "main": "projects/my-portfolio/src/main.ts",
            "polyfills": "projects/my-portfolio/src/polyfills.ts",
            "tsConfig": "projects/my-portfolio/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/my-portfolio/src/favicon.ico",
              "projects/my-portfolio/src/assets"
            ],
            "styles": [
              "projects/my-portfolio/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/my-portfolio/src/environments/environment.ts",
                  "with": "projects/my-portfolio/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "my-portfolio:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "my-portfolio:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "my-portfolio:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/my-portfolio/src/test.ts",
            "polyfills": "projects/my-portfolio/src/polyfills.ts",
            "tsConfig": "projects/my-portfolio/tsconfig.spec.json",
            "karmaConfig": "projects/my-portfolio/karma.conf.js",
            "assets": [
              "projects/my-portfolio/src/favicon.ico",
              "projects/my-portfolio/src/assets"
            ],
            "styles": [
              "projects/my-portfolio/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/my-portfolio/tsconfig.app.json",
              "projects/my-portfolio/tsconfig.spec.json",
              "projects/my-portfolio/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/my-portfolio/e2e/protractor.conf.js",
            "devServerTarget": "my-portfolio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-portfolio:serve:production"
            }
          }
        }
      }
    },
    "my-cms": {
